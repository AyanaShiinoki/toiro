			<% exhibition.works.each do |work| %>
				<div class = "col xl4 l6 m12 s12">
					<div class ="center-align work-image"><%= attachment_image_tag work, :work_image, :fill, 300, 300 ,class: "z-depth-4"%></div>
					<h5 class ="center-align"><%= work.work_title %></h5>
					<p class ="center-align"><%= work.description %></p>
				</div>
			<% end %>