<div class="container">
	<h1>フォルダ詳細</h1>
		<%= @folder.folder_name %>
		<%= @folder.user.name %>
		<% @clips.each do |clip| %>
			<h5><%= clip.exhibition.id %></h5>
			<%= link_to "#{clip.exhibition.title}", users_exhibition_path(clip.exhibition) %>
			<%= clip.memo %>
			<%= link_to "クリップを外す", users_exhibition_clips_path(clip.exhibition),method: :delete %><br>
		<% end %>
</div>